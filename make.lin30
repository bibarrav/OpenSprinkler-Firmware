SKETCH = ./examples/mainArduino/mainArduino.ino
LIBS = .\
  $(ESP_LIBS)/Wire \
  $(ESP_LIBS)/ESP8266WiFi \
  $(ESP_LIBS)/ESP8266WebServer \
  $(ESP_LIBS)/ESP8266mDNS \
  $(ESP_LIBS)/SSD1306 \
  $(ESP_LIBS)/rc-switch \

ESP_ROOT = $(HOME)/workspace/esp8266_2.4/
BUILD_ROOT = /tmp/$(MAIN_NAME)

UPLOAD_SPEED = 115200
UPLOAD_VERB = -v
UPLOAD_RESET = nodemcu

FLASH_DEF = 4M3M
FLASH_MODE = dout
FLASH_SPEED = 40 
F_CPU = 160000000L

BOARD = generic

include ./makeEspArduino.mk
